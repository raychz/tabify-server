{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Backend",
            "port": 9229,
            "preLaunchTask": "docker",
            "postDebugTask": "post_debug_kill_docker",
            "restart": true,
            "skipFiles": ["<node_internals>/**", "**/*.js"],
            "runtimeExecutable": "npm",
            "console": "integratedTerminal",
            "presentation": {
                "hidden": false,
                "group": "backend"
            },
            "runtimeArgs": [
                "run", "debug"
            ]
        },
    ],
    "compounds": [
        {
            "name": "Fullstack",
            "configurations": ["Backend", "Frontend"]
        }
    ]
}