{
  "name": "tabify-server",
  "version": "0.0.1",
  "description": "description",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "cross-env NODE_ENV=development nodemon",
    "start:debug": "cross-env NODE_ENV=development nodemon --config nodemon-debug.json",
    "prestart:staging": "rimraf dist && yarn build",
    "start:staging": "cross-env NODE_ENV=staging node -r ./tsconfig-paths-bootstrap.js dist/main.js",
    "prestart:prod": "rimraf dist && yarn build",
    "start:prod": "cross-env NODE_ENV=production node -r ./tsconfig-paths-bootstrap.js dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "webpack": "webpack --config webpack.config.js",
    "preinstall": "node -e 'if(!/yarn\\.js$/.test(process.env.npm_execpath)) throw new Error(\"Use yarn\")'"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "dependencies": {
    "@nest-middlewares/morgan": "^5.1.1",
    "@nestjs/common": "^5.1.0",
    "@nestjs/core": "^5.1.0",
    "@types/dotenv": "^4.0.3",
    "@types/dotenv-flow": "^0.2.1",
    "@types/node-fetch": "^2.1.2",
    "cross-env": "^5.2.0",
    "currency.js": "^1.2.2",
    "dotenv-flow": "^0.2.0",
    "express": "^4.16.4",
    "firebase": "^5.8.2",
    "firebase-admin": "^7.0.0",
    "mysql": "^2.16.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "6",
    "rxjs-compat": "6",
    "socket.io": "^2.2.0",
    "typeorm": "^0.2.19",
    "typescript": "3.4.5"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.7.1",
    "@types/socket.io": "^2.1.2",
    "@types/supertest": "^2.0.5",
    "jest": "^23.6.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "tsconfig-paths": "^3.8.0",
    "tslint": "5.11.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  }
}
